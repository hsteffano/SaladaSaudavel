        <activity
            android:name="com.project.saladaSaudavel.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.project.saladaSaudavel.HomeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.project.saladaSaudavel.RegistroActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.project.saladaSaudavel.PedidosActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.project.saladaSaudavel.DadosActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.project.saladaSaudavel.NutricionalActivity"
